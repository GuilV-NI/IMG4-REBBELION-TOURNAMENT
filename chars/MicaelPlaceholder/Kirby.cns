trigger1 = Time = 0
y = -1.3

;Controllable jump.
[State 50, MoveFwd]
type = VelSet
triggerall = Alive
trigger1 = Command = "holdfwd"
X = IfElse(Vel X < Const(velocity.jump.fwd.x), Vel X+0.4, Const(velocity.jump.fwd.x))

[State 50, MoveBack]
type = VelSet
triggerall = Alive
trigger1 = Command = "holdback"
X = IfElse(Vel X > -Const(velocity.jump.fwd.x), Vel X-0.4, -Const(velocity.jump.fwd.x))

;Don't move.
[State 50, VelCancel]
type = VelMul
triggerall = Alive
trigger1 = Command != "holdfwd"
trigger1 = Command != "holdback"
trigger1 = Vel X != 0
X = IfElse(Abs(Vel X) > 0.3, fvar(3), 0)

[State 1312, 2]
